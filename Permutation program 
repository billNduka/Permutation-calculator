function factorial(n){
  let result = 1
  for(let i=n; i > 0; i--){
    result = result*i
  }
  return (result)
}

function permute(n,r){
  let answer = factorial(n)/factorial(n-r)
  console.log(answer)
}

let a = Number(window.prompt("what number?"))
let b = Number(window.prompt("how many places?"))

permute(a,b)